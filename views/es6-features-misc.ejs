    <section id="array">
        <h3>New Array Featur<span style="font-weight:bold;color:#333;background-color:yellow;">ES</span></h3>
        <div class="fragment">
            <p><code>Array.from</code></p>
            <pre><code class="js">
    Array.from('BAE');// ['B', 'A', 'E']
            </code></pre>
        </div>
        <div class="fragment">
            <p><code>Array.prototype.fill</code></p>
            <pre><code class="js">
    let glass = ['empty', 'empty', 'empty'];
    glass.fill('water');// glass === ['water', 'water', 'water']
            </code></pre>
        </div>
        <div class="fragment">
            <p><code>Array.prototype.find</code></p>
            <pre><code class="js">
    [1, 2, 3].find(isEven);// 2
    [1, 2, 3].findIndex(isEven);// 1
            </code></pre>
        </div>
